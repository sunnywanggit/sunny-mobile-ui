.s-card__header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 10rpx 20rpx 10rpx 24rpx;
  height: 60rpx;
  position: relative;
}
.s-card__header::before {
  content: '';
  display: inline-block;
  height: 6rpx;
  width: 6rpx;
  background-color: red;
  position: absolute;
  left: 8rpx;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.s-card__label {
  font-size: 26rpx;
  color: #909ca2;
}
.s-card__container {
  background-color: #fff;
}
.s-card__footer {
  border-top: 1rpx solid #eee;
  padding: 10rpx;
}

