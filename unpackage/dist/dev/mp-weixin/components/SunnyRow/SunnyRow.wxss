.s-row {
  position: relative;
  box-sizing: border-box;
}
.s-row::after, .s-row::before {
  content: "";
  display: table;
}
.s-row::after {
  clear: both;
}
.s-row--flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.s-row--flex::after, .s-row--flex::before {
  display: none;
}
.s-row.is-justify-center {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.s-row.is-justify-end {
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.s-row.is-justify-space-between {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.s-row.is-justify-space-around {
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.s-row.is-align-middle {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.s-row.is-align-bottom {
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
}
.s-row.is-wrap {
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}

